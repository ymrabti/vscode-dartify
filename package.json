{
    "name": "ymrabti.dartifyjson",
    "displayName": "DARTIFY: JSON to DART data model",
    "description": "Advanced JSON to Dart models generator. Generates null-safe classes, Flutter Forms, Enums, Extensions, and States. Supports nested structures and lists.",
    "version": "1.0.1",
    "author": {
        "name": "Younes MRabti",
        "email": "admin@youmti.net",
        "url": "https://avatars.githubusercontent.com/ymrabti"
    },
    "license": "MIT",
    "release": {
        "github": {
            "owner": "ymrabti",
            "repo": "vscode-dartify"
        }
    },
    "markdown": "github",
    "exports": {
        ".": "./extension.js"
    },
    "scripts": {
        "test-dart-class": "node src/example-dart-class",
        "preprebi": "echo \"Pre build install\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prebi": "vsce package -o outs --no-yarn",
        "bi": "code --install-extension outs/dartifyjson-1.0.0.vsix",
        "prepublish": "yarn run prebi",
        "publish": "ovsx publish",
        "test": "node ./test/runTest.js",
        "test:unit": "mocha test/unit/**/*.test.js --ui tdd"
    },
    "engines": {
        "vscode": "^1.77.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ymrabti/vscode-dartify"
    },
    "bugs": {
        "url": "https://github.com/ymrabti/vscode-dartify/issues"
    },
    "publisher": "YOUMTINET",
    "icon": "assets/dartify_04.jpg",
    "categories": [
        "Other",
        "Snippets",
        "Programming Languages"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "dartify.parseJson",
                "title": "DARTIFY: Generate classes",
                "shortTitle": "dartify-genclasses"
            }
        ],
        "keybindings": [
            {
                "command": "dartify.parseJson",
                "key": "ctrl+shift+alt+t"
            }
        ]
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.70.0",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^9.28.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.13.0",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-security": "^3.0.1",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "ovsx": "^0.10.8",
        "typescript": "^4.7.4"
    },
    "_moduleAliases": {
        "@root": ".",
        "@src": "src",
        "@outs": "outs"
    },
    "dependencies": {
        "module-alias": "2.2.3"
    },
    "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
    "keywords": [
        "dart",
        "flutter",
        "json",
        "generator",
        "models",
        "forms",
        "json-to-dart"
    ]
}